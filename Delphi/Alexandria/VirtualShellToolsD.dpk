package VirtualShellToolsD;

{$R *.res}
{$IFDEF IMPLICITBUILDING This IFDEF should not be used by users}
{$ALIGN 8}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DEFINE RELEASE}
{$ENDIF IMPLICITBUILDING}
{$DESCRIPTION 'VirtualShellTools Delphi Runtime Package'}
{$LIBSUFFIX AUTO}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  rtl,
  vcl,
  vclimg,
  EasyListviewD,
  MPCommonLibD,
  VirtualTreesDR;

contains
  AppletsAndWizards in '..\..\Source\AppletsAndWizards.pas',
  VirtualCommandLine in '..\..\Source\VirtualCommandLine.pas',
  VirtualExplorerTree in '..\..\Source\VirtualExplorerTree.pas',
  VirtualRedirector in '..\..\Source\VirtualRedirector.pas',
  VirtualResources in '..\..\Source\VirtualResources.pas',
  VirtualScrollbars in '..\..\Source\VirtualScrollbars.pas',
  VirtualSendToMenu in '..\..\Source\VirtualSendToMenu.pas',
  VirtualShellAutoComplete in '..\..\Source\VirtualShellAutoComplete.pas',
  VirtualShellHistory in '..\..\Source\VirtualShellHistory.pas',
  VirtualShellNewMenu in '..\..\Source\VirtualShellNewMenu.pas',
  VirtualShellNotifier in '..\..\Source\VirtualShellNotifier.pas',
  VirtualShellToolBar in '..\..\Source\VirtualShellToolBar.pas',
  VirtualThumbnails in '..\..\Source\VirtualThumbnails.pas',
  VirtualExplorerEasyListview in '..\..\Source\VirtualExplorerEasyListview.pas',
  VirtualExplorerEasyListModeview in '..\..\Source\VirtualExplorerEasyListModeview.pas',
  VirtualFileSearch in '..\..\Source\VirtualFileSearch.pas',
  ColumnForm in '..\..\Source\ColumnForm.pas' {FormColumnSettings};

end.
